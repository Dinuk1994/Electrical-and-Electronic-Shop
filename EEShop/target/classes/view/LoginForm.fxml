<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.MotionBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFormController">
   <children>
      <ImageView fitHeight="810.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@../img/HD-wallpaper-pos-system-vectors-stock-psd-pos-machine.jpg" />
         </image>
         <effect>
            <MotionBlur />
         </effect>
      </ImageView>
      <JFXTextField fx:id="txtEmail" alignment="TOP_LEFT" layoutX="84.0" layoutY="410.0" prefHeight="38.0" prefWidth="430.0" promptText="Enter Email Address">
         <font>
            <Font size="19.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtPassword" layoutX="86.0" layoutY="503.0" prefHeight="31.0" prefWidth="430.0" promptText="Enter Password">
         <font>
            <Font size="19.0" />
         </font>
      </JFXTextField>
      <Label fx:id="lblDate" alignment="CENTER" layoutX="5.0" layoutY="4.0" prefHeight="57.0" prefWidth="141.0" text="YYYY-mm-dd">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Label>
      <JFXButton buttonType="RAISED" layoutX="209.0" layoutY="595.0" onAction="#userLoginBtnOnAction" prefHeight="57.0" prefWidth="166.0" style="-fx-background-color: #605DCA;" text="Login" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </JFXButton>
      <JFXButton buttonType="RAISED" layoutX="163.0" layoutY="668.0" onAction="#btnForgotPassword" prefHeight="57.0" prefWidth="259.0" text="Forgot Password ?" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </JFXButton>
      <Label alignment="CENTER" layoutX="75.0" layoutY="67.0" prefHeight="100.0" prefWidth="450.0" text="LOGIN FORM">
         <font>
            <Font name="Bodoni MT Bold" size="59.0" />
         </font>
      </Label>
      <ImageView fitHeight="800.0" fitWidth="624.0" layoutX="600.0" layoutY="10.0" pickOnBounds="true">
         <image>
            <Image url="@../img/login-account-with-password-and-unlock-security-free-vector.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="728.0" layoutY="225.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="lblTime" alignment="CENTER" layoutX="432.0" prefHeight="64.0" prefWidth="166.0" text="HH:mm:ss">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Label>
      <ImageView fitHeight="198.0" fitWidth="220.0" layoutX="176.0" layoutY="167.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/computer-icons-login-user-system-administrator-image-admin-removebg-preview.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
